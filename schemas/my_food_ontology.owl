    <!-- http://my_food_ontology.org/AlimOntology#DataQualityRating -->

    <owl:ObjectProperty rdf:about="http://my_food_ontology.org/AlimOntology#DataQualityRating">
        <rdfs:subPropertyOf rdf:resource="http://my_food_ontology.org/AlimOntology#EnvironmentIndicators"/>
        <rdfs:domain rdf:resource="http://my_food_ontology.org/AlimOntology#foodItem"/>
        <rdfs:range rdf:resource="http://my_food_ontology.org/AlimOntology#quantityValue"/>
        <rdfs:comment>A data quality score ranging from 1 (very good) to 5 (very poor). It is a reliability score associated with each agricultural and food product for which Agribalyse provides life cycle inventories and impact indicators. This score is calculated according to the method recommended by the European Commission and considers several criteria: temporal representativeness (TiR), technological representativeness (TeR), precision (P), and geographic specificity (GR).</rdfs:comment>
        <rdfs:label>Data Quality Rating</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://my_food_ontology.org/AlimOntology#EFSingleScore -->

    <owl:ObjectProperty rdf:about="http://my_food_ontology.org/AlimOntology#EFSingleScore">
